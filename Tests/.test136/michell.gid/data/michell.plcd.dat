#####################################################
#####################################################
$------------------------------------------------------------
PHYSICAL_PROBLEM
$------------------------------------------------------------ 
PROBLEM_DEFINTION  
NUMBER OF STAGES: 1
BEGIN STAGE
   NUMBER OF SUBSTAGES:        1
   BEGIN SUBSTAGE
      Initial_Load_Factor: 1
      Final_Load_Factor:   1
      Time_Step:           1
      Time_Interval:       1
      Maximum_NonLinear_Iterations: 1
      Iterations_Tolerance:         0.001
   END_SUBSTAGE
END_STAGE
 TOPOLOGY_OPTIMIZATION: TOPOLOGICAL_DERIVATIVE STOCHASTIC
      ALPHA: 5
      LEVEL: 2
      FUNCTION_FORCES  
      END_FUNCTION_FORCES
      SIMULTANEOUS_LOADS: YES
   END_STOCHASTIC_DATA
   CHIMIN:           1e-3
   CHIMAX:           1.0
   VOLUME_FRACTION:  0.4
   CHI_SPACE:        1
   KPLUS    :        1.5
   KMINUS    :        0.5
   KPOWER    :        1
 END_TOPOLOGY_OPTIMIZATION

END_PROBLEM_DEFINITION
$------------------------------------------------------------
PROPERTIES
INCLUDE michell.plcd.mat
END_PROPERTIES  
$------------------------------------------------------------
END_PHYSICAL_PROBLEM  
$------------------------------------------------------------
NUMERICAL_TREATMENT
   SMOOTH_DISPLACEMENT_GRADIENTS:  OFF
   UP_FORMULATION:                 OFF
   STA_CONSTANTS:                  1
$  STEADY_STATE_TOLER:    1e-8   
$  NORM_OF_CONVERGENCE:   L2   
$  CONVERGENCE_TOLERANCE: 1e-6
$  MAXIMUM_NUMBER_OF_IT:  100
ERROR_ESTIMATOR:         LAYERS, GENERAL: 0, INTERFACE: 4, NLAYERS: 2  
END_NUMERICAL_TREATMENT  
$------------------------------------------------------------
OUTPUT_&_POST-PROCESS  
 START_POSTPROCES_AT                STEP =1
 POSTPROCESS DISPLACEMENT,          STEPS=1
 POSTPROCESS STRESS,                STEPS=1
 POSTPROCESS STRAIN,                STEPS=1
 POSTPROCESS MAT_DATA,              STEPS=0
 POSTPROCESS MAT_DATA, ITERATIONS: OFF
 POSTPROCESS SUBSCALES,             STEPS=0
 POSTPROCESS TOPOL_DERIV           STEPS 1 
 PNTBC:                               No
 PLCD_Postprocess_MatData_Iteration
  PNTBC: No
END_OUTPUT_&_POST_PROCESS  
$------------------------------------------------------------
BOUNDARY_CONDITIONS, \
      DONT_FIX_PRESSURE
  INCLUDE michell.plcd.fix
END_BOUNDARY_CONDITIONS  
