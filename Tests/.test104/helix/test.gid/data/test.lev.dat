$------------------------------------------------------------
PHYSICAL_PROBLEM
$------------------------------------------------------------
PROBLEM_DEFINITION
  TEMPORAL_DERIVATIVES:   On 
  EXACT_LEVELSET:         Off          
  ADVECTION:         Off            
END_PROBLEM_DEFINITION
$------------------------------------------------------------
PROPERTIES:
END_PROPERTIES
$------------------------------------------------------------
END_PHYSICAL_PROBLEM
$------------------------------------------------------------
NUMERICAL_TREATMENT
 STABILITY_CONSTANTS=     4.0 2.0 1.0
 INIT_REDISTANCE:         On
 REINITIALIZATION_SCHEME: POISSON
 NSTEPS_REINITIALIZATION: 1e6
 MASS_CORRECTION:         Off
 NoALE:                          Default
  TIME_ACCURACY:         1   
  TIME_INTEGRATION:      BDF   
  SAFETY_FACTOR:         1e24  
  STEADY_STATE_TOLER:    -1e-8  
 NORM_OF_CONVERGENCE:     L2   
 MAXIMUM_NUMBER_OF_ITER=  1  
 CONVERGENCE_TOLERANCE=   1e-8 
 ERROR_ESTIMATOR:         LAYERS, PARA1: 5, PARA2: 0, PARA3: 0
END_NUMERICAL_TREATMENT
$------------------------------------------------------------
OUTPUT_&_POST_PROCESS
 START_POSTPROCES_AT            STEP =1
 POSTPROCESS LEVELSET,          STEPS=1
END_OUTPUT_&_POST-PROCESS
$------------------------------------------------------------
BOUNDARY_CONDITIONS \
      CONSTANT_BOUNDARY_CONDITIONS
 INCLUDE test.lev.fix
END_BOUNDARY_CONDITIONS
$------------------------------------------------------------
