$------------------------------------------------------------
PHYSICAL_PROBLEM
$------------------------------------------------------------ 
PROBLEM_DEFINTION  
NUMBER OF STAGES: 1
BEGIN STAGE
   NUMBER OF SUBSTAGES:        1
   BEGIN SUBSTAGE
      Initial_Load_Factor: 0.0
      Final_Load_Factor:   1
      Time_Step:           40
      Time_Interval:       120
      Maximum_NonLinear_Iterations: 3
      Iterations_Tolerance:         0.001
   END_SUBSTAGE
END_STAGE
END_PROBLEM_DEFINITION
$------------------------------------------------------------
PROPERTIES
INCLUDE Plate_J2_PlaneStress.plcd.mat
END_PROPERTIES  
$------------------------------------------------------------
END_PHYSICAL_PROBLEM  
$------------------------------------------------------------
NUMERICAL_TREATMENT
   SMOOTH_DISPLACEMENT_GRADIENTS:  ON
   UP_FORMULATION:                 ON
   STA_CONSTANTS:                  1
$  STEADY_STATE_TOLER:    1e-8   
$  NORM_OF_CONVERGENCE:   L2   
$  CONVERGENCE_TOLERANCE: 1e-8
$  MAXIMUM_NUMBER_OF_IT:  1
$  LINEARIZATION_METHOD:  RHS, PICARD_ITERATIONS: 0  
 ERROR_ESTIMATOR:         MAT_COMPARISON_CRITERIA, MAX_ERROR: 1.3, MIN_ERROR: 0.7, CRITERIA: Tolerance, MAX_REFLEVEL: 3
END_NUMERICAL_TREATMENT  
$------------------------------------------------------------
OUTPUT_&_POST-PROCESS  
  START_POSTPROCES_AT               STEP =1
 POSTPROCESS DISPLACEMENT,          STEPS=1
 POSTPROCESS STRESS,                STEPS=1
 POSTPROCESS MAT_DATA,              STEPS=1
  POSTPROCESS MAT_DATA, ITERATIONS: OFF
 POSTPROCESS SUBSCALES,             STEPS=0 
 PLCD_Postprocess_MatData_Iteration
END_OUTPUT_&_POST_PROCESS  
$------------------------------------------------------------
BOUNDARY_CONDITIONS
  INCLUDE Plate_J2_PlaneStress.plcd.fix
END_BOUNDARY_CONDITIONS  
