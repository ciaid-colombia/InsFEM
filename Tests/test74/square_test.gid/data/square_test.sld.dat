$------------------------------------------------------------
PHYSICAL_PROBLEM
$------------------------------------------------------------ 
PROBLEM_DEFINTION  
  TEMPORAL_DERIVATIVES: On  
  GRAVITY:              NORM: 9.81, \
                       GX:   0.0, \
                       GY:   -1.0, \
                       GZ:   0.0  
END_PROBLEM_DEFINITION
$------------------------------------------------------------
PROPERTIES
  SLDMODEL = LINEAR
  DENSITY=          7850  
  YOUNG=            200e9  
  POISSON=          0.3  
  DAMPING(%)        0.0
END_PROPERTIES  
$------------------------------------------------------------
END_PHYSICAL_PROBLEM  
$------------------------------------------------------------
NUMERICAL_TREATMENT
  T2_SCHEME                 NEWMA $ {BDF1,BDF2,NEWMA}
  T2_EULER_STEPS            2 $ number of backward euler steps, 0 by default
  SAFETY_FACTOR:         1e24  
  STEADY_STATE_TOLER:    1e-8   
  NORM_OF_CONVERGENCE:   L2   
  CONVERGENCE_TOLERANCE: 1e-8
  MAXIMUM_NUMBER_OF_IT:  100
END_NUMERICAL_TREATMENT  
$------------------------------------------------------------
OUTPUT_&_POST-PROCESS  
 START_POSTPROCES_AT         STEP =1
 POSTPROCESS DISPLACEMENT,   STEPS=1
 POSTPROCESS STRESS,         STEPS=1
 POSTPROCESS STRAIN,         STEPS=1
END_OUTPUT_&_POST_PROCESS  
$------------------------------------------------------------
BOUNDARY_CONDITIONS, \
 CONSTANT_BOUNDARY_CONDITIONS
  INCLUDE square_test.sld.fix
END_BOUNDARY_CONDITIONS  
$------------------------------------------------------------
